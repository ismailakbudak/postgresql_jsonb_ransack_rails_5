<div class="panel panel-default">
  <div class="panel-heading">
    User fields
    <div class="pull-right">
      <%= link_to 'New', new_user_fields_path(user, resource) %>
    </div>
  </div>
  <div class="panel-body">
    <% if fields.present? %>
        <ul>
          <% fields.keys.each do |key| %>
              <li>
                <%= fields[key]['name'] %>
                <%= link_to 'Delete', delete_user_fields_path(user, resource, key), method: :delete %>
                <%= link_to 'Edit', edit_user_fields_path(user, resource, key) %>
                <ul>
                  <li>Type: <%= fields[key]['type'] %></li>
                  <li>Show filter? : <%= fields[key]['is_filter_exist'] %></li>
                  <li>Show sort link? : <%= fields[key]['is_sort_exist'] %></li>
                </ul>
              </li>
          <% end %>
        </ul>
    <% end %>
  </div>
</div>